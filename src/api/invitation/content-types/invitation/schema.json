{
  "kind": "collectionType",
  "collectionName": "invitations",
  "info": {
    "singularName": "invitation",
    "pluralName": "invitations",
    "displayName": "Invitation",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "message": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "event": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::evento.evento"
    },
    "guest_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "status": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "pending",
        "accepted",
        "rejected"
      ],
      "default": "pending"
    },
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "default": "Has sido invitado a un evento!!"
    }
  }
}
